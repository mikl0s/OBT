# OBT server URL (required)
OBT_SERVER_URL=http://localhost:8881

# Ollama API URL (optional, default: http://localhost:11434)
OLLAMA_URL=http://localhost:11434

# Client ID (optional, default: default-client)
CLIENT_ID=my-client

# Heartbeat interval in seconds (optional, default: 10)
HEARTBEAT_INTERVAL=10
